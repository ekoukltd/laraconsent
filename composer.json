{
    "name": "ekoukltd/laraconsent",
    "description": "User consent module for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Evans",
            "email": "lee@ekouk.com",
            "homepage": "https://www.ekouk.com"
        }
    ],
    "homepage": "https://github.com/ekoukltd/laraconsent",
    "keywords": ["Laravel", "LaraConsent","GDPR","ISO27001","User Consent"],
    "require": {
        "illuminate/support": "~7|~8",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "~9.0",
        "ekoukltd/laravel-admin-tools": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ekoukltd\\LaraConsent\\": "src/",
            "Ekoukltd\\LaraConsent\\Database\\Factories\\": "database/factories",
            "Ekoukltd\\LaraConsent\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekoukltd\\LaraConsent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ekoukltd\\LaraConsent\\LaraConsentServiceProvider"
            ],
            "aliases": {
                "LaraConsent": "Ekoukltd\\LaraConsent\\Facades\\LaraConsent"
            }
        }
    }
}
